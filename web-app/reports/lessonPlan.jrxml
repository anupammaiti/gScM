	<queryString>
		<![CDATA[SELECT lesson.exam_term, lesson.week_start_date, lesson.week_end_date,  section.name as sectionName, lesson.week_number as weekNumber, employee.name as empName,
lesson.lesson_date as lessonDate, subject.name as subjectName,
lesson_details.topics, lesson_details.home_work, lesson_details.exam_date
FROM lesson
join lesson_details on lesson.id=lesson_details.lesson_id
inner join section on lesson.section_id=section.id
inner join employee on lesson_details.employee_id=employee.id
inner join subject_name subject on lesson_details.subject_id=subject.id
where $P!{sqlParam}
order by lesson.week_number asc, lesson.lesson_date asc, subject.sort_position asc]]>
	</queryString>
