	<parameter name="sqlParams" class="java.lang.String"/>
	<queryString>
		<![CDATA[select ex.name exam, cs.name className, se.name sctionName, sn.name subject,
(select count(*) from v_std_student_subject where section_id = se.id and subject_id=sn.id) numOfStd,
(select count(id) from exam_mark where exam_id=exs.exam_id and exam_schedule_id=exs.id and ct_attend_status !='NO_INPUT' and active_status='ACTIVE'
 and student_id in (select std_id from v_std_student_subject where section_id = se.id and subject_id=sn.id)) ctEntryCount,
(select count(id) from exam_mark where exam_id=exs.exam_id and exam_schedule_id=exs.id and hall_attend_status !='NO_INPUT' and active_status='ACTIVE'
 and student_id in (select std_id from v_std_student_subject where section_id = se.id and subject_id=sn.id)) hallEntryCount,
exs.is_ct_mark_input, exs.is_hall_mark_input
from exam_schedule exs
inner join exam ex on ex.id= exs.exam_id and ex.exam_status='NEW' and ex.active_status='ACTIVE'
inner join class_name cs on cs.id = ex.class_name_id
inner join section se on se.id= ex.section_id
inner join subject_name sn on sn.id = exs.subject_id
where $P!{sqlParams}  and exs.active_status='ACTIVE'
order by cs.sort_position, se.id, sn.sort_position]]>
	</queryString>
