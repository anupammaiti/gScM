	<queryString>
		<![CDATA[select std.id as id, std.studentid, std.name, std.roll_no, tabu.attendance_day attend_day,
	tabu.result_status, tabu.grand_total_mark, tabu.g_point, tabu.l_grade, tabu.section_str_position, tabu.class_str_position,
	cs.subject_name subject, emark.ct_attend_status, cs.ct_mark as ct_exam_mark, cs.ct_eff_mark ct_mark_eff_percentage, emark.ct_obtain_mark, emark.ct_mark as cmark,
	emark.hall_attend_status, cs.hall_mark as hall_exam_mark, cs.hall_eff_mark hall_mark_eff_percentage, emark.hall_mark, emark.is_optional, emark.hall_written_mark, emark.hall_objective_mark, emark.hall_practical_mark, emark.hall_sba_mark, emark.hall_input5,
	emark.tabulation_mark as oTotalMark, cs.weight_on_result tabulation_eff_percentage,
	emark.g_point as gPoint, emark.l_grade as lGrade, exsch.highest_mark, tabu.total_obtain_mark as tom,
	(IFNULL(cs.hall_mark, 0) * cs.hall_eff_mark * 0.01) as hallEMark, (IFNULL(cs.ct_mark, 0) * cs.ct_eff_mark * 0.01) as ctEMark
	from exam_mark emark
	inner join student std on emark.student_id=std.id
inner join exam_schedule exsch on emark.exam_schedule_id=exsch.id
	inner join tabulation tabu on emark.student_id=tabu.student_id AND emark.exam_id=tabu.exam_id
inner join v_std_class_subject cs on cs.class_name_id =std.class_name_id and cs.subject_id=exsch.subject_id and cs.subject_id in (SELECT subject_id FROM v_std_student_subject where subject_type in ('COMPULSORY','OPTIONAL','INUSE') and std_id = std.id)
where (emark.exam_id=$P{examId} and emark.active_status='ACTIVE' and emark.student_id in ($P!{stdIDs}))
order by std.roll_no, cs.sort_position]]>
	</queryString>
